  
function latitude= getLatitudeUSBGPS(dataIn,directionIn)
 
	parfor n = 1:length(dataIn)

		latitude(n) = double((vpa(dataIn(n)) - round(vpa(dataIn(n))/100)*100)/60+round(dataIn(n)/100))

	  	 if directionIn(n) == 'S'
        		latitude(n) = latitude(n) *-1; 
   	   	 end

	end   
latitude = array2table(latitude)
end



